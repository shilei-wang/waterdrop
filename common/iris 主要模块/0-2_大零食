import internal.GlobalVariable as GlobalVariable
import utils.CmdUtils
import utils.StringUtils as StringUtils


//CmdUtils.pl(response.responseBodyContent)
TestData faucet = findTestData('base/faucet')
v0 = faucet.getValue('name', 1)
v0_iaa = CmdUtils.getAddressFromName(v0, "iaa")
command =GlobalVariable.app+"query account".concat(GlobalVariable.chainId).concat(GlobalVariable.node).concat(GlobalVariable.json).concat(" "+v0_iaa)

response = CmdUtils.sendRequest('cmd/CmdWithOneArgs', command, "sync")
re = CmdUtils.Parse(response.responseBodyContent).getAsJsonObject()
address = re.get("address").getAsString()
CmdUtils.pl(address)

WS.verifyEqual(address, v0_iaa)

