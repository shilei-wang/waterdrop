echo 1234567890 | iriscli bank send --amount=10000iris --fee=0.004iris --commit --chain-id=shilei-qa --from=v0 --to=faa1lcuw6ewd2gfxap37sejewmta205sgssmv5fnju




echo 1234567890 | iriscli stake delegate --address-validator=fva1pj09ca4h80x5lcw6ep8vfwuu4ws9y2g00hpna0 --amount=5iris --fee=0.004iris --commit  --from=v0  --chain-id=shilei-qa


